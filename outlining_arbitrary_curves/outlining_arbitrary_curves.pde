void setup() {
  size(360, 360); 
  stroke(0);
  noFill();
  
  // alternate between the bg color and fg color 
  
  color foreground_color = color(89, 46, 117);
  color background_color = color(240, 237, 242);
  
  for (int j = 60; j >= 1; j -= 1) {
    strokeWeight(7 * (j));
    
    if (j % 2 == 0) {
      stroke(foreground_color);
    } else {
      stroke(background_color);
    }
    
    beginShape();
    for(int i = 0; i < r_coords.length; i += 12) {
      curveVertex(r_coords[i] - 60, r_coords[i + 1] + 20);
    }
    endShape();
  }
  
  stroke(foreground_color);
  strokeWeight(2);
    beginShape();
    for(int i = 0; i < r_coords.length; i += 12) {
      curveVertex(r_coords[i] - 60, r_coords[i + 1] + 20);
    }
    endShape();
}

